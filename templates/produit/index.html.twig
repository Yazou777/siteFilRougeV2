{% extends 'base.html.twig' %}

{% block title %}produit{% endblock %}

{% block body %}
<h1 class="text-center my-5"> Nos produit </h1>

<div class="container">
    <div class="row ">
{% for f in findby %}

        <div class="col-12 col-sm-12 col-md-6 col-lg-4 text-center my-5 " >
       
            <a href="#">
             <div>{{ f.pronom }}</div>
                <img alt="{{ f.pronom }} image non disponible" class="imageCategorie"src="/images/{{ f.proimage }}"> 
             
                </img>
            </a>
             <div>{{ f.proprix|number_format(2, '.', ',') }} euro</div>
        </div>
{% endfor %}
    </div>
</div>
{% endblock %}